{
    "version": "0.1.0",
    "command": "miktex-pdflatex",
    "isShellCommand": true,
    "args": [
        "-interaction=nonstopmode",
        "-file-line-error",
        "HWs/LX/LX331_A4.tex"
    ],
    "showOutput": "silent",
    "problemMatcher": {
        "owner": "latex-preview",
        "fileLocation": [
            "relative",
            "${workspaceRoot}"
        ],
        "pattern": {
            "regexp": "^(.*):(\\d+):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "message": 3
        }
    }
}